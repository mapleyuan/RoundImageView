<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="maskType">
            <enum name="RECTANGLE" value="0"/>
            <enum name="CIRCLE" value="1"/>
            <enum name="ROUNDRECTANGLE" value="2"/>
            <enum name="ROUNDRECTANGLETOP" value="3"/>
        </attr>
        <attr name="borderWidth_Ri" format="dimension"/>
        <attr name="borderColor_Ri" format="color|reference"/>
    </declare-styleable>
</resources>